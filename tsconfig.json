{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/WeatherLoader.jsx",
    "src/components/Loader.jsx",
    "src/components/PDFBitacora.jsx",
    "src/components/SVGComponent.jsx",
    "src/components/Footer.jsx",
    "src/components/AnalysisLoader.jsx",
    "src/components/LoginForm.tsx",
    "src/app/soporte/page.jsx"
, "src/app/proceso/consultar/equipo/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
